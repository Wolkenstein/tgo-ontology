     -->

    <rdf:Description rdf:about="http://www.hinkelmanns.at/tgo/Border">
        <rdfs:comment xml:lang="en">A border marks a start or end point of a text</rdfs:comment>
    </rdf:Description>
    <rdf:Description rdf:about="http://www.hinkelmanns.at/tgo/Connector">
        <rdfs:comment xml:lang="en">A connector is a rdf node used to represent a weighted connection between two nodes</rdfs:comment>
    </rdf:Description>
    <rdf:Description rdf:about="http://www.hinkelmanns.at/tgo/End">
        <rdfs:comment xml:lang="en">The ending point of of a text</rdfs:comment>
    </rdf:Description>
    <rdf:Description rdf:about="http://www.hinkelmanns.at/tgo/Node">
        <rdfs:comment xml:lang="en">A Node is the base class for all nodes of the text graph.</rdfs:comment>
    </rdf:Description>
    <rdf:Description rdf:about="http://www.hinkelmanns.at/tgo/Path">
        <rdfs:comment xml:lang="en">A Path is a route from one node to another via their connections. and can be defined by start and end points and waypoints</rdfs:comment>
    </rdf:Description>
    <rdf:Description rdf:about="http://www.hinkelmanns.at/tgo/Start">
        <rdfs:comment xml:lang="en">The starting point of of a text</rdfs:comment>
    </rdf:Description>
    <rdf:Description rdf:about="http://www.hinkelmanns.at/tgo/Token">
        <rdfs:comment xml:lang="en">A Token is a seperated string, mostly seperated by whitespace in the source document.</rdfs:comment>
    </rdf:Description>
    <rdf:Description rdf:about="http://www.w3.org/ns/oa#hasBody">
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/oa#"/>
        <rdfs:comment>The object of the relationship is a resource that is a body of the Annotation.</rdfs:comment>
    </rdf:Description>
