    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.hinkelmanns.at/tgo/Border -->

    <owl:Class rdf:about="http://www.hinkelmanns.at/tgo/Border">
        <rdfs:subClassOf rdf:resource="http://www.hinkelmanns.at/tgo/Node"/>
    </owl:Class>
    


    <!-- http://www.hinkelmanns.at/tgo/Connector -->

    <owl:Class rdf:about="http://www.hinkelmanns.at/tgo/Connector">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.hinkelmanns.at/tgo/hasNext"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.hinkelmanns.at/tgo/Node"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.hinkelmanns.at/tgo/hasPreceding"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.hinkelmanns.at/tgo/Node"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.hinkelmanns.at/tgo/weight"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.hinkelmanns.at/tgo/End -->

    <owl:Class rdf:about="http://www.hinkelmanns.at/tgo/End">
        <rdfs:subClassOf rdf:resource="http://www.hinkelmanns.at/tgo/Border"/>
    </owl:Class>
    


    <!-- http://www.hinkelmanns.at/tgo/Grapheme -->

    <owl:Class rdf:about="http://www.hinkelmanns.at/tgo/Grapheme">
        <rdfs:subClassOf rdf:resource="http://www.hinkelmanns.at/tgo/Node"/>
    </owl:Class>
    


    <!-- http://www.hinkelmanns.at/tgo/GraphemeConnector -->

    <owl:Class rdf:about="http://www.hinkelmanns.at/tgo/GraphemeConnector">
        <rdfs:subClassOf rdf:resource="http://www.hinkelmanns.at/tgo/Connector"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.hinkelmanns.at/tgo/hasNext"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.hinkelmanns.at/tgo/Grapheme"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.hinkelmanns.at/tgo/hasPreceding"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.hinkelmanns.at/tgo/Grapheme"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.hinkelmanns.at/tgo/Group -->

    <owl:Class rdf:about="http://www.hinkelmanns.at/tgo/Group">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.hinkelmanns.at/tgo/hasMember"/>
                <owl:someValuesFrom rdf:resource="http://www.hinkelmanns.at/tgo/Node"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.hinkelmanns.at/tgo/Morpheme -->

    <owl:Class rdf:about="http://www.hinkelmanns.at/tgo/Morpheme">
        <rdfs:subClassOf rdf:resource="http://www.hinkelmanns.at/tgo/Node"/>
    </owl:Class>
    


    <!-- http://www.hinkelmanns.at/tgo/MorphemeConnector -->

    <owl:Class rdf:about="http://www.hinkelmanns.at/tgo/MorphemeConnector">
        <rdfs:subClassOf rdf:resource="http://www.hinkelmanns.at/tgo/Connector"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.hinkelmanns.at/tgo/hasNext"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.hinkelmanns.at/tgo/Morpheme"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.hinkelmanns.at/tgo/hasPreceding"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.hinkelmanns.at/tgo/Morpheme"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.hinkelmanns.at/tgo/Node -->

    <owl:Class rdf:about="http://www.hinkelmanns.at/tgo/Node"/>
    


    <!-- http://www.hinkelmanns.at/tgo/Path -->

    <owl:Class rdf:about="http://www.hinkelmanns.at/tgo/Path">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.hinkelmanns.at/tgo/hasEnd"/>
                <owl:someValuesFrom rdf:resource="http://www.hinkelmanns.at/tgo/Node"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.hinkelmanns.at/tgo/hasStart"/>
                <owl:someValuesFrom rdf:resource="http://www.hinkelmanns.at/tgo/Node"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.hinkelmanns.at/tgo/Phrase -->

    <owl:Class rdf:about="http://www.hinkelmanns.at/tgo/Phrase">
        <rdfs:subClassOf rdf:resource="http://www.hinkelmanns.at/tgo/Group"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.hinkelmanns.at/tgo/hasMember"/>
                <owl:someValuesFrom rdf:resource="http://www.hinkelmanns.at/tgo/Token"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.hinkelmanns.at/tgo/Sentence -->

    <owl:Class rdf:about="http://www.hinkelmanns.at/tgo/Sentence">
        <rdfs:subClassOf rdf:resource="http://www.hinkelmanns.at/tgo/Path"/>
    </owl:Class>
    


    <!-- http://www.hinkelmanns.at/tgo/Start -->

    <owl:Class rdf:about="http://www.hinkelmanns.at/tgo/Start">
        <rdfs:subClassOf rdf:resource="http://www.hinkelmanns.at/tgo/Border"/>
    </owl:Class>
    


    <!-- http://www.hinkelmanns.at/tgo/Text -->

    <owl:Class rdf:about="http://www.hinkelmanns.at/tgo/Text">
        <rdfs:subClassOf rdf:resource="http://www.hinkelmanns.at/tgo/Path"/>
    </owl:Class>
    


    <!-- http://www.hinkelmanns.at/tgo/TextStage -->

    <owl:Class rdf:about="http://www.hinkelmanns.at/tgo/TextStage">
        <rdfs:subClassOf rdf:resource="http://www.hinkelmanns.at/tgo/Text"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.hinkelmanns.at/tgo/usesConnector"/>
                <owl:someValuesFrom rdf:resource="http://www.hinkelmanns.at/tgo/Connector"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.hinkelmanns.at/tgo/Token -->

    <owl:Class rdf:about="http://www.hinkelmanns.at/tgo/Token">
        <rdfs:subClassOf rdf:resource="http://www.hinkelmanns.at/tgo/Node"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.hinkelmanns.at/tgo/hasEnd"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.hinkelmanns.at/tgo/Morpheme"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.hinkelmanns.at/tgo/hasStart"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.hinkelmanns.at/tgo/Morpheme"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#label"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.hinkelmanns.at/tgo/TokenConnector -->

    <owl:Class rdf:about="http://www.hinkelmanns.at/tgo/TokenConnector">
        <rdfs:subClassOf rdf:resource="http://www.hinkelmanns.at/tgo/Connector"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.hinkelmanns.at/tgo/hasNext"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.hinkelmanns.at/tgo/Token"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.hinkelmanns.at/tgo/hasPreceding"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.hinkelmanns.at/tgo/Token"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.hinkelmanns.at/tgo/Word -->

    <owl:Class rdf:about="http://www.hinkelmanns.at/tgo/Word">
        <rdfs:subClassOf rdf:resource="http://www.hinkelmanns.at/tgo/Group"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.hinkelmanns.at/tgo/hasMember"/>
                <owl:someValuesFrom rdf:resource="http://www.hinkelmanns.at/tgo/Token"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


