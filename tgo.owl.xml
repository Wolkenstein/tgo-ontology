    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.hinkelmanns.at/tgo/hasEnd -->

    <owl:ObjectProperty rdf:about="http://www.hinkelmanns.at/tgo/hasEnd">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:range rdf:resource="http://www.hinkelmanns.at/tgo/Node"/>
        <dc:description xml:lang="en">hasEnd connects a Connector or a Path with its ending node</dc:description>
    </owl:ObjectProperty>
    


    <!-- http://www.hinkelmanns.at/tgo/hasMember -->

    <owl:ObjectProperty rdf:about="http://www.hinkelmanns.at/tgo/hasMember">
        <rdfs:range rdf:resource="http://www.hinkelmanns.at/tgo/Token"/>
    </owl:ObjectProperty>
    


    <!-- http://www.hinkelmanns.at/tgo/hasNext -->

    <owl:ObjectProperty rdf:about="http://www.hinkelmanns.at/tgo/hasNext">
        <owl:inverseOf rdf:resource="http://www.hinkelmanns.at/tgo/hasPreceding"/>
        <rdfs:range rdf:resource="http://www.hinkelmanns.at/tgo/Node"/>
    </owl:ObjectProperty>
    


    <!-- http://www.hinkelmanns.at/tgo/hasPreceding -->

    <owl:ObjectProperty rdf:about="http://www.hinkelmanns.at/tgo/hasPreceding">
        <rdfs:range rdf:resource="http://www.hinkelmanns.at/tgo/Node"/>
    </owl:ObjectProperty>
    


    <!-- http://www.hinkelmanns.at/tgo/hasStart -->

    <owl:ObjectProperty rdf:about="http://www.hinkelmanns.at/tgo/hasStart">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:range rdf:resource="http://www.hinkelmanns.at/tgo/Node"/>
        <dc:description xml:lang="en">hasStart connects a Connector or a Path with its starting node</dc:description>
    </owl:ObjectProperty>
    


    <!-- http://www.hinkelmanns.at/tgo/usesConnector -->

    <owl:ObjectProperty rdf:about="http://www.hinkelmanns.at/tgo/usesConnector">
        <rdfs:domain rdf:resource="http://www.hinkelmanns.at/tgo/Path"/>
        <rdfs:range rdf:resource="http://www.hinkelmanns.at/tgo/Connector"/>
    </owl:ObjectProperty>
    


